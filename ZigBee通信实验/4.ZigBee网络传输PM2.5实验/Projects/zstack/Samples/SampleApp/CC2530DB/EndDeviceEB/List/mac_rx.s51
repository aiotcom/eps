//  215   rxFifoOverflowCount  = 0;
        MOV     DPTR,#rxFifoOverflowCount
        MOVX    @DPTR,A
//  216 }
        POP     DPH
        CFI DPH0 SameValue
        CFI CFA_SP SP+-4
        POP     DPL
        CFI DPL0 SameValue
        CFI CFA_SP SP+-3
