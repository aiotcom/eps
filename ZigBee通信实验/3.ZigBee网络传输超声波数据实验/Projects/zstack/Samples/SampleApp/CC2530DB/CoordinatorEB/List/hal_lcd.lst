    843          /**************************************************************************************************
    844           * @fn      HalLcd_HW_WaitUs
    845           *
    846           * @brief   wait for x us. @ 32MHz MCU clock it takes 32 "nop"s for 1 us delay.
    847           *
    848           * @param   x us. range[0-65536]
    849           *
    850           * @return  None
    851           **************************************************************************************************/
    852          void HalLcd_HW_WaitUs(uint16 microSecs)
    853          {
    854            while(microSecs--)
    855            {
    856              /* 32 NOPs == 1 usecs */
    857              asm("nop"); asm("nop"); asm("nop"); asm("nop"); asm("nop");
    858              asm("nop"); asm("nop"); asm("nop"); asm("nop"); asm("nop");
    859              asm("nop"); asm("nop"); asm("nop"); asm("nop"); asm("nop");
    860              asm("nop"); asm("nop"); asm("nop"); asm("nop"); asm("nop");
    861              asm("nop"); asm("nop"); asm("nop"); asm("nop"); asm("nop");
    862              asm("nop"); asm("nop"); asm("nop"); asm("nop"); asm("nop");
    863              asm("nop"); asm("nop");
    864            }
    865          }
