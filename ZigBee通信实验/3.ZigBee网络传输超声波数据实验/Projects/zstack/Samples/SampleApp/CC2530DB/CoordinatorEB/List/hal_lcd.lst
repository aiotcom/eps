    815          /**************************************************************************************************
    816           * @fn          halLcdWriteLine
    817           *
    818           * @brief       Write one line on display
    819           *
    820           * @param       uint8 line - display line
    821           *              char *pText - text buffer to write
    822           *
    823           * @return      none
    824           **************************************************************************************************/
    825          void HalLcd_HW_WriteLine(uint8 line, const char *pText)
    826          {
    827            uint8 count;
    828            uint8 totalLength = (uint8)osal_strlen( (char *)pText );
    829          
    830            /* Write the content first */
    831            for (count=0; count<totalLength; count++)
    832            {
    833              HalLcd_HW_WriteChar(line, count, (*(pText++)));
    834            }
    835          
    836            /* Write blank spaces to rest of the line */
    837            for(count=totalLength; count<LCD_MAX_LINE_LENGTH;count++)
    838            {
    839              HalLcd_HW_WriteChar(line, count, ' ');
    840            }
    841          }
