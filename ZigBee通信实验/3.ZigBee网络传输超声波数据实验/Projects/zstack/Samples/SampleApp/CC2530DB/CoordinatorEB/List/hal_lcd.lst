    329          /**************************************************************************************************
    330           * @fn      HalLcdWriteString
    331           *
    332           * @brief   Write a string to the LCD
    333           *
    334           * @param   str    - pointer to the string that will be displayed
    335           *          option - display options
    336           *
    337           * @return  None
    338           **************************************************************************************************/

   \                                 In  segment BANKED_CODE, align 1, keep-with-next
    339          void HalLcdWriteString ( char *str, uint8 option)
   \                     HalLcdWriteString:
    340          {
   \   000000                ; Saved register size: 0
   \   000000                ; Auto size: 0
    341          #if (HAL_LCD == TRUE)
    342          
    343            uint8 strLen = 0;
    344            uint8 totalLen = 0;
    345            uint8 *buf;
    346            uint8 tmpLen;
    347          
    348            if ( Lcd_Line1 == NULL )
    349            {
    350              Lcd_Line1 = osal_mem_alloc( HAL_LCD_MAX_CHARS+1 );
    351              HalLcdWriteString( "TexasInstruments", 1 );
    352            }
    353          
    354            strLen = (uint8)osal_strlen( (char*)str );
    355          
    356            /* Check boundries */
    357            if ( strLen > HAL_LCD_MAX_CHARS )
    358              strLen = HAL_LCD_MAX_CHARS;
    359          
    360            if ( option == HAL_LCD_LINE_1 )
    361            {
    362              /* Line 1 gets saved for later */
    363              osal_memcpy( Lcd_Line1, str, strLen );
    364              Lcd_Line1[strLen] = '\0';
    365            }
    366            else
    367            {
    368              /* Line 2 triggers action */
    369              tmpLen = (uint8)osal_strlen( (char*)Lcd_Line1 );
    370              totalLen =  tmpLen + 1 + strLen + 1;
    371              buf = osal_mem_alloc( totalLen );
    372              if ( buf != NULL )
    373              {
    374                /* Concatenate strings */
    375                osal_memcpy( buf, Lcd_Line1, tmpLen );
    376                buf[tmpLen++] = ' ';
    377                osal_memcpy( &buf[tmpLen], str, strLen );
    378                buf[tmpLen+strLen] = '\0';
    379          
    380                /* Send it out */
    381          #if defined (ZTOOL_P1) || defined (ZTOOL_P2)
    382          
    383          #if defined(SERIAL_DEBUG_SUPPORTED)
    384                debug_str( (uint8*)buf );
    385          #endif //LCD_SUPPORTED
    386          
    387          #endif //ZTOOL_P1
    388          
    389                /* Free mem */
    390                osal_mem_free( buf );
    391              }
    392            }
    393          
    394            /* Display the string */
    395            HalLcd_HW_WriteLine (option, str);
    396          
    397          #endif //HAL_LCD
    398          
    399          }
