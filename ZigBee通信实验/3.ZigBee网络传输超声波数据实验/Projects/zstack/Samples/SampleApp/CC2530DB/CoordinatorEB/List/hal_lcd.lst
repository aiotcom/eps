    737          /**************************************************************************************************
    738           * @fn      HalLcd_HW_WriteChar
    739           *
    740           * @brief   Write one char to the display
    741           *
    742           * @param   uint8 line - line number that the char will be displayed
    743           *          uint8 col - colum where the char will be displayed
    744           *
    745           * @return  None
    746           **************************************************************************************************/
    747          void HalLcd_HW_WriteChar(uint8 line, uint8 col, char text)
    748          {
    749              uint8 j;
    750              uint16 addr;
    751              uint8 cvt_result;
    752          
    753              if (col < LCD_MAX_LINE_LENGTH && line <= HAL_LCD_LINE_4)
    754              {
    755                      
    756                  //write first line 
    757                  SET_DDRAM_ADDR(col * 8, (line - 1) * 2);
    758                  addr = (text - 0x20)*12;
    759          
    760                  for (j = 0; j < 8; j++)
    761                  {
    762                       cvt_result = HalLcd_Convert_DZK(j,addr);
    763                       cvt_result = cvt_result << 2;
    764                  	HalLcd_HW_Write(cvt_result);
    765                  }
    766          
    767                  //write second line 
    768                  SET_DDRAM_ADDR(col * 8, (line - 1) * 2 + 1);
    769                  addr = addr + 6;
    770          
    771                  for (j = 0; j < 8; j++)
    772                  {
    773                       cvt_result = HalLcd_Convert_DZK(j,addr);
    774                  	HalLcd_HW_Write(cvt_result);
    775                  }
    776          
    777              }
    778              else
    779              {
    780                  return;
    781              }
    782          }
    783          
    784          uint8 HalLcd_Convert_DZK(uint8 index, uint16 addr)
    785          {
    786              uint8 return_val = 0;
    787              uint8 j;
    788              uint8 temp;
    789              uint8 mask_array[8] = {BIT7,BIT6,BIT5,BIT4,BIT3,BIT2,BIT1,BIT0};
    790              uint8 mask = mask_array[index];
    791          
    792              if (index > 7)
    793              {
    794                  return return_val;
    795              }
    796          
    797              for (j = 0; j < 6; j++)
    798              {
    799                  temp = ASCIITAB_DZK[addr + j];
    800                  temp = temp & mask;
    801                  temp = temp >> (7 - index);
    802                  if (temp)
    803                  {
    804                      return_val |=  mask_array[7-j];
    805                  }
    806                  else
    807                  {
    808                      return_val &=  ~mask_array[7-j];
    809                  }
    810              }
    811          
    812              return return_val;
    813          }
